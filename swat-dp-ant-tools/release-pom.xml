  <build>
    <sourceDirectory>src/main/java</sourceDirectory>
    <scriptSourceDirectory>D:\srdev\workspaces\dp\swat-dp-ant-tools\src\main\scripts</scriptSourceDirectory>
    <testSourceDirectory>src/test/java</testSourceDirectory>
    <outputDirectory>target/classes</outputDirectory>
    <testOutputDirectory>target/test-classes</testOutputDirectory>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>src/test/resources</directory>
      </testResource>
    </testResources>
    <directory>target</directory>
    <finalName>swat-dp-ant-tools-1.0.3</finalName>
  </build>
  <reporting>
    <outputDirectory>target/site</outputDirectory>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>taglist-maven-plugin</artifactId>
        <version>2.4</version>
        <configuration>
          <tagListOptions>
            <tagClasses>
              <tagClass>
                <displayName>Todo Work</displayName>
                <tags>
                  <tag>
                    <matchString>todo</matchString>
                    <matchType>ignoreCase</matchType>
                  </tag>
                  <tag>
                    <matchString>FIXME</matchString>
                    <matchType>exact</matchType>
                  </tag>
                  <tag>
                    <matchString>@todo</matchString>
                    <matchType>exact</matchType>
                  </tag>
                  <tag>
                    <matchString>@deprecated</matchString>
                    <matchType>exact</matchType>
                  </tag>
                </tags>
              </tagClass>
              <tagClass>
                <displayName>Special Tags</displayName>
                <tags>
                  <tag>
                    <matchString>foobar</matchString>
                    <matchType>ignoreCase</matchType>
                  </tag>
                  <tag>
                    <matchString>pshah</matchString>
                    <matchType>exact</matchType>
                  </tag>
                </tags>
              </tagClass>
            </tagClasses>
          </tagListOptions>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-project-info-reports-plugin</artifactId>
        <version>2.4</version>
      </plugin>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.8.1</version>
        <configuration>
          <source>1.5</source>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-report-plugin</artifactId>
        <version>2.12</version>
      </plugin>
      <plugin>
        <artifactId>maven-jxr-plugin</artifactId>
        <version>2.3</version>
        <configuration>
          <bottom>Copyright (c) 2012 schlag&amp;rahm AG. All Rights Reserved.</bottom>
          <destDir>D:\srdev\workspaces\dp\swat-dp-ant-tools\target/site/xref</destDir>
          <docTitle>ANT tools for the Swat4DP module 1.0.3-SNAPSHOT Reference</docTitle>
          <inputEncoding>ISO-8859-1</inputEncoding>
          <javadocDir>D:\srdev\workspaces\dp\swat-dp-ant-tools\target/site/apidocs</javadocDir>
          <outputDirectory>D:\srdev\workspaces\dp\swat-dp-ant-tools\target/site</outputDirectory>
          <outputEncoding>ISO-8859-1</outputEncoding>
          <project>${project}</project>
          <stylesheet>stylesheet.css</stylesheet>
          <templateDir>templates</templateDir>
          <windowTitle>ANT tools for the Swat4DP module 1.0.3-SNAPSHOT Reference</windowTitle>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-changelog-plugin</artifactId>
        <version>2.2</version>
        <reportSets>
          <reportSet>
            <id>single-report</id>
            <reports>
              <report>changelog</report>
            </reports>
            <configuration>
              <type>range</type>
              <range>30</range>
            </configuration>
          </reportSet>
        </reportSets>
      </plugin>
    </plugins>
  </reporting>
