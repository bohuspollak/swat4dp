    <dp:request domain="@domain.name@" xmlns:dp="http://www.datapower.com/schemas/management">
      <dp:do-action>
        <AddPasswordMap>
          <AliasName>@password.alias.name@</AliasName>
          <Password>@password.alias.secret@</Password>
        </AddPasswordMap>
      </dp:do-action>
    </dp:request>
  </soapenv:Body>
</soapenv:Envelope>